<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<%
String username = (String)session.getAttribute( "userName" );
if(username==null||username==""){
	response.sendRedirect("login.jsp");
}
%>
<meta http-equiv="refresh" content="<%= session.getMaxInactiveInterval() %>;url=/nbpx/admin/login.jsp">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<title>南北培训网后台管理系统</title>
	<link rel="stylesheet" type="text/css" href="../js/easyui/themes/gray/easyui.css">
	<link rel="stylesheet" type="text/css" href="../js/easyui/themes/icon.css"/>
	<script type="text/javascript" src="../js/easyui/jquery-1.8.0.min.js"></script>
	<script type="text/javascript" src="../js/easyui/jquery.easyui.min.js"></script>
	<script type="text/javascript">
	function open1(title,plugin){
		if ($('#tt').tabs('exists',plugin)){
			$('#tt').tabs('select', plugin);
		} else {
			$('#tt').tabs('add',{
				title:title,
				href:plugin+'.html',
				closable:true
			});
		}
	}


	</script>
	<style>
		dd{font-size:12px;padding:0;margin:0;padding-left:30px;padding-top:8px;height:12px;}
		dd a{text-decoration:none;color:#333}
		dd a:hover{text-decoration:underline;color:red}
		.userInfor{float:right;padding-right:20px}
		.userInfor ul{list-style:none}
		.userInfor li{float:left;display:inline;padding-left:10px;}
		.userInfor li a,.userInfor li span{color:white;text-decoration:none;font-size:12px;}
		.userInfor li a:hover{text-decoration:underline}
	</style>
</head>

<body class="easyui-layout" style="min-width:1000px;overflow:auto">
	<div region="north" style="height:60px;line-height:60px;border:none;overflow:hidden;font-weight:normal;font-size:16px;background:url(../images/adminLogoBg.jpg) repeat-x">
		<img src="../images/adminLogo.jpg" style='float:left'></img>
		<div class="userInfor"><ul><li><span>当前用户:<%=username%></span></li><li><a href="#">修改个人信息</a></li><li><a href="#">退出登陆</a></li></ul></div>
	</div>

	<div region="west" split="true" style="width:220px;overflow:hidden" title="功能菜单">
			<ul class="easyui-tree">
				<li iconCls="icon-base"><span>系统管理</span>
					<ul>
						<li iconCls="icon-gears">
							<a href="#" onclick="javascript:open1('系统日志')">系统日志</a>
						</li>
						
						<li iconCls="icon-gears">
							<a href="#" onclick="javascript:open1('广告设置')">广告设置</a>
						</li>
					</ul>
				</li>
				<li iconCls="icon-base"><span>课程管理</span>
					<ul>
						<li iconCls="icon-gears">
							<a href="javascript:open1('发布公开课','addCourse')">发布公开课</a>
						</li>
						
						<li iconCls="icon-gears">
							<a href="javascript:open1('公开课管理','courses')">公开课管理</a>
						</li>
						<li iconCls="icon-gears">
							<a href="javascript:open1('内训课管理','innerCourses')">内训课管理</a>
						</li>
						<li iconCls="icon-gears">
							<a href="javascript:open1('需求管理','requirements')">需求管理</a>
						</li>
						<li iconCls="icon-gears">
							<a href="javascript:open1('报名管理','applications')">报名管理</a>
						</li>
					</ul>
				</li>
				
				<li iconCls="icon-base"><span>资讯管理</span>
					<ul>
						<li iconCls="icon-gears">
							<a href="javascript:open1('文章管理','articles')">文章管理</a>
						</li>
						
						<li iconCls="icon-gears">
							<a href="javascript:open1('现场管理','liveScenes')">现场管理</a>
						</li>
						<li iconCls="icon-gears">
							<a href="javascript:open1('拓展管理','tuozhan')">拓展管理</a>
						</li>
						<li iconCls="icon-gears">
							<a href="javascript:open1('下载管理','downloads')">下载管理</a>
						</li>
					</ul>
				</li>
				
				<li iconCls="icon-base"><span>问答管理</span>
					<ul>
						<li iconCls="icon-gears">
							<a href="javascript:open1('问答管理','wendas')">问答管理</a>
						</li>
						
					</ul>
				</li>
				
				<li iconCls="icon-base"><span>资料管理</span>
					<ul>
						<li iconCls="icon-gears">
							<a href="javascript:void(0)" onclick="javascript:open1('外部注册讲师资料管理','outTeacherList')">外部注册讲师资料管理</a>
						</li>
						<li iconCls="icon-gears">
							<a href="javascript:void(0)" onclick="javascript:open1('内部发布讲师资料管理','teacherList')">内部发布讲师资料管理</a>
						</li>
						<li iconCls="icon-gears">
							<a href="javascript:void(0)" onclick="javascript:open1('机构资料管理','orgList')">机构资料管理</a>
						</li>
						<li iconCls="icon-gears">
							<a href="javascript:void(0)" onclick="javascript:open1('企业资料管理','compList')">企业资料管理</a>
						</li>
					</ul>
				</li>
				
				<li iconCls="icon-base"><span>系统设置</span>
					<ul>
						<li iconCls="icon-gears">
							<a href="javascript:void(0)" onclick="javascript:open1('字典管理','dictionary')">字典管理</a>
						</li>
						
						<li iconCls="icon-gears">
							<a href="javascript:void(0)" onclick="javascript:open1('关键词管理','keywords')">关键词管理</a>
						</li>
						
						<li iconCls="icon-gears">
							<a href="javascript:void(0)" onclick="javascript:open1('专题管理','subjects')">专题管理</a>
						</li>
						
						<li iconCls="icon-gears">
							<a href="javascript:void(0)" onclick="javascript:open1('系统用户','userList')">系统用户</a>
						</li>
					</ul>
				</li>
			</ul>
	</div>

	<div region="center" border="false">
			<div id="tt" class="easyui-tabs" fit="true" border="false" plain="true">
				<div title="首页">
					<p style='padding:20px;padding-left:20px'><%=username%>,欢迎登陆南北培训网后台管理系统</p>
				</div>
			</div>
	</div>

	<div id="menu" class="easyui-menu" style="width:150px;">
		<div id="m-refresh">刷新</div>
		<div class="menu-sep"></div>
		<div id="m-closeall">全部关闭</div>
		<div id="m-closeother">除此之外全部关闭</div>
		<div class="menu-sep"></div>
		<div id="m-close">关闭</div>
	</div>
</body>
</html>


